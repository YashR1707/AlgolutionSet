// Reverse Array
#include<bits/stdc++.h>
using namespace std;

int main()
{
    cout<<"Enter 7 Integers in the Array";
    int a[7];
    for(int i=0;i<7;i++)
    {
        cin>>a[i];
    }
    
    int i=0,j=6,temp;
    while(i < j)
    {
        temp = a[i];
        a[i] = a[j];
        a[j] = temp;
        i++,j--;
        
    }
    
    for(i=0;i<7;i++)
    {
        cout<<a[i]<<" ";
    }
  

    return 0;
}

//Merge of 2 sorted array
#include<bits/stdc++.h>
using namespace std;

int main()
{
    int n;
    cout<<"Enter size of Arrays :";
    cin>>n;
    
    int a[n],b[n];
    int m = n+n;
    int c[m];
    cout<<"Enter elements in array 1 :" ;
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
    
     cout<<"Enter elements in array 2 :" ;
    for(int i=0;i<n;i++)
    {
        cin>>b[i];
    }
    
    int i=0,j=0;
    
    int k=0;
   while(i<n || j<n)
   {
       if(a[i]<b[j])
       {
           c[k]=a[i];
           i++,k++;
       }
       else 
       {
           c[k]=b[j];
           j++,k++;
       }
      
   }
   
   cout<<"Final Sorted "; 
   for(int k=0;k<m;k++)
   {
       cout<<c[k]<<" ";
   }  
    return 0;
}

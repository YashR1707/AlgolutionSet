#include <iostream>
using namespace std;

int main() {
    int arr[] = { 2, 2, 0, -9, 6, 7 };
    int n = sizeof(arr) / sizeof(int);
    int max = arr[0]; // Initialize min with the first element
    for (int i = 1; i < n; i++) {
        if (arr[i] > max) {
            max = arr[i]; // Update min if current element is smaller
  }
    }
    
    cout << "Maximum element: " << max<< endl;
    
    return 0;
}

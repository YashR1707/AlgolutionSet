#include <iostream>
#include <map>
using namespace std;

int main() {
    int n;
    cout << "Enter the size of the array: ";
    cin >> n;
    
    int arr[n];
    cout << "Enter " << n << " elements: ";
    for (int i = 0; i < n; i++) {
        cin >> arr[i];
    }
    
    map<int, int> frequency;
    int duplicateCount = 0;
    
    for (int i = 0; i < n; i++) {
        frequency[arr[i]]++;
    }
    
    for (auto it : frequency) {
        if (it.second > 1) {
            duplicateCount++;
        }
    }
    
    cout << "Total number of duplicate numbers: " << duplicateCount << endl;
    
    return 0;
}
